      "id":"technology/2013/oct/13/jake-humphrey-me-and-my-tech",
      "sectionId":"technology",
      "sectionName":"Technology",
      "webPublicationDate":"2013-10-13T07:00:01Z",
      "webTitle":"Jake Humphrey: me and my tech",
      "webUrl":"http://www.theguardian.com/technology/2013/oct/13/jake-humphrey-me-and-my-tech",
      "apiUrl":"http://content.guardianapis.com/technology/2013/oct/13/jake-humphrey-me-and-my-tech",
