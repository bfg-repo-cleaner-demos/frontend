      "id":"sport/blog/2013/oct/17/australia-rugby-union-player-wages",
      "sectionId":"sport",
      "sectionName":"Sport",
      "webPublicationDate":"2013-10-17T22:52:00Z",
      "webTitle":"The ARU must speculate to accumulate",
      "webUrl":"http://www.theguardian.com/sport/blog/2013/oct/17/australia-rugby-union-player-wages",
      "apiUrl":"http://content.guardianapis.com/sport/blog/2013/oct/17/australia-rugby-union-player-wages",
